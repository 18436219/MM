package com.difu.view;

import java.awt.FlowLayout;

import javax.swing.*;

public class DiFu extends JFrame{
	   JLabel ja1;
	   JButton ja2;
	   JPanel ja3;
	   
	public DiFu(){
		ja1=new JLabel(new ImageIcon("tupian/timg.jpg"));
		this.getContentPane().setLayout(null);
		ja1.setBounds(0,0,340,350);
		this.getLayeredPane().add(ja1,new Integer(Integer.MIN_VALUE));
		ja2=new JButton("jaskdfjals");
		this.getContentPane().setLayout(new FlowLayout());
		ja3=(JPanel)this.getContentPane();
		ja3.add(ja2);
		ja3.setOpaque(false);
		

		this.setSize(340,350);
		this.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		this.setLocationRelativeTo(null);
		this.setVisible(true);
	}
	
	public static void main(String[] args) {
		DiFu difu=new DiFu();
	}

}
